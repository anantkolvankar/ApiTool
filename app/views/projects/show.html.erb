<p id="notice"><%= notice %></p>
<div class='col-md-3'>
	<blockquote>
		<h2><%= @project.name %></h2>
		<p>Base URl : <%= @project.base_url %></p>
	</blockquote>
	<div class='panel-group'>
		<div class='panel panel-default'>
			<div class='panel-heading clearfix'>
				<div class='pull-left'>
					<h4 class='panel-title'>Services</h4>
				</div>
				<div class="pull-right">
					<%= link_to 'Add Service', new_project_service_path(params[:id]), remote: true %>
				</div>
			</div>
			<div class = 'panel-collapse collapse in'>
				<div class = 'panel-body'>
					<% @project.services.each do |project_service| %>
						<ul class = 'list-unstyled'>
							<li>
								<%= link_to project_service.name, project_service_path(@project, project_service), remote: true %>
							</li>
						</ul>
					<% end %>
				</div>
			</div>
		</div>

		<div class='panel panel-default'>
			<div class ='panel-heading'>
				<h4 class ='panel-title'>
					Objects
				</h4>
			</div>
			<div class = 'panel-collapse collapse in', style='display:none'>
				<div class = 'panel-body'>
					<% @project.entities.each do |project_entity| %>
						<ul class = 'list-unstyled'>
							<li>
								<%= link_to project_entity.name, '' %>
							</li>
						</ul>
					<% end %>
				</div>
			</div>
		</div>
	</div>

</div>
<div class='col-md-9'>
	<div class='services-content'>
	</div>
</div>
</div>
